{
  "name": "top-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "npm run build && electron-builder --config electron-builder.config.json",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "electron": "concurrently --kill-others \"vite dev\" \"electron src/electron.cjs\"",
    "dev:package": "npm run build && electron-builder --config electron-builder.config.json --dir"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "electron-builder": "^23.1.0",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mdsvex": "^0.10.6",
    "nprogress": "^0.2.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "~4.7.2",
    "vite": "^2.9.14"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.8",
    "@lukeed/uuid": "^2.0.0",
    "@poppanator/sveltekit-svg": "^0.3.2",
    "@supabase/supabase-js": "^1.35.3",
    "cookie": "^0.5.0",
    "daisyui": "^2.15.2",
    "electron": "^19.0.6",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "idb": "^7.0.2",
    "theme-change": "^2.0.2"
  }
}
